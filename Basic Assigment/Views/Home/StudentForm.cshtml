
@{
    ViewData["Title"] = "StudentForm";
}

<table id="maimTable">
    <tr>
        <td>
            <form onsubmit="event.preventDefault();onFormSubmit();" autocomplete="off">
                <div>
                    <label>Full Name*</label><label class="validation-error hide" id="fullNameValidationError">This field is required.</label>
                    <input type="text" name="fullName" id="fullName">
                </div>
                <div>

                    <label>Father Name</label><label class="validation-error hide" id="fthNameValidationError">This field is required.</label>
                    <input type="text" name="FthName" id="FthName">
                </div>
                <div>
                    <label>CNIC</label><label class="validation-error hide" id="CNICvalidationError">This field is required.</label>
                    <input type="text" name="CNIC" id="CNIC">
                </div>
                <div>
                    <label>Course</label><label class="validation-error hide" id="courseValidationError">This field is required.</label>
                    <input type="text" name="course" id="course">
                </div>
                <div>
                    <label>Gender</label><label class="validation-error hide" id="courseValidationError">This field is required.</label>
                    <input type="text" name="course" id="course">
                </div>
                <div class="form-action-buttons">
                    <input type="submit" value="Submit">
                </div>
            </form>
        </td>
        </tr>
</table>
<br />
<br />
<table class="list" id="studentList">
    <thead>
        <tr>
            <th>Full Name</th>
            <th>Father Name</th>
            <th>CNIC</th>
            <th>Course</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
    </tbody>
</table>
<link href="~/css/studentFormcss.css" rel="stylesheet" />
<script src="~/js/studentForm.js"></script>



